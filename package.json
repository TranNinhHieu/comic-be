{
  "name": "comic-riverdev-api",
  "version": "1.0.0",
  "description": "APIs for comic clone app",
  "main": "src/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint src",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel ./src -d ./build/src",
    "build": "npm run clean && npm run build-babel",
    "production": "npm run build && node ./build/src/server.js",
    "dev": "nodemon --exec ./node_modules/.bin/babel-node ./src/server.js",
    "start": "node ./build/src/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/river248/comic-riverdev-api.git"
  },
  "keywords": [],
  "author": "river",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/river248/comic-riverdev-api/issues"
  },
  "homepage": "https://github.com/river248/comic-riverdev-api#readme",
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "google-auth-library": "^7.10.1",
    "googleapis": "^91.0.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.1.3",
    "nodemailer": "^6.7.1",
    "socket.io": "^4.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "eslint": "^8.0.1",
    "nodemon": "^2.0.19"
  }
}
